@model IEnumerable<ShopBanChuot.Models.NHOMTAIKHOAN>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<h2>Quản lý nhóm tài khoản</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenNhomTK)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenNhomTK)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaNhomTK }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaNhomTK }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaNhomTK })
        </td>
    </tr>
}

</table>
